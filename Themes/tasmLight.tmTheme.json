{
    "type": "light",
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#594A4A",
        "editorIndentGuide.background": "#404040",
        "editorRuler.foreground": "#333333",
        "activityBarBadge.background": "#007acc",
        "sideBarTitle.foreground": "#bbbbbb"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings":{
                "foreground":"#7AA04B"
            }
        },
        {
            "scope": "constant.numeric",
            "settings":{
                "foreground":"#D18100"
            }
        },
        {
            "scope": "string.quoted.single",
            "settings":{
                "foreground":"#5C9962"
            }
        },
        {
            "scope": "string.quoted.double",
            "settings":{
                "foreground":"#914040"
            }
        },
        {
            "scope": "keyword",
            "settings":{
                "foreground": "#066289",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.less",
            "settings":{
                "foreground": "#4275B3",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.less",
            "settings":{
                "foreground": "#289D9F",
                "fontStyle":"none"
            }
        },
        {
            "scope": "keyword.size",
            "settings":{
                "foreground": "#28289F"             
            }
        },
        {
            "scope": "variable.register",
            "settings":{
                "foreground": "#4B65B4",
                "background": "FFFF85",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.valued",
            "settings":{
                "foreground": "#636363",
                "fontStyle":"none"
            }
        }
    ]
}